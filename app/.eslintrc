{
  "extends": "airbnb-base",
  "root": true,
  "rules": {
    "object-curly-spacing": ["error", "never"],
    "strict": 0,
    "max-len": 0,
    "no-underscore-dangle": 0,
    "curly": 0,
    "prefer-template": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "no-empty": 0,
    "new-parens": 0,
    "no-console": 0,
    "no-cond-assign": 0,
    "no-fallthrough": 0,
    "new-cap": 0,
    "no-loop-func": 0,
    "no-unreachable": 0,
    "no-process-exit": 0,
    "quotes": [1, "single", "avoid-escape"]
  },
  "env": {
    "node": true,
    "es6": true
  }
}
